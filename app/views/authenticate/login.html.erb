<h1>Please Log In</h1>
  <%= form_tag do %>
    <p><%= email_field_tag :email, nil, placeholder: "Email" %></p>
    <p><%= password_field_tag :password, nil, placeholder: "Password" %></p>
    <%= submit_tag "Login Teacher", user_type: "Teacher" %>
    <%= submit_tag "Login Student", user_type: "Student"%>
    <%= submit_tag "Login Parent", user_type: "Parent"%>
  <% end %>
